@page
@model webApi.Pages.Boardmember.IndexModel

@{
    ViewData["Title"] = "Index";
     Layout = "~/Areas/Identity/Pages/Account/Admin/_Layout.cshtml";
}

<div class="cont">
    <div class="container">
        <div>
            <h1>Create Boardmember</h1>
        </div>
    <p>
        <a asp-page="Create">Create New BoardMember</a>
    </p>
    </div>
</div>
<div class="cart container">
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.BoardMember[0].Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BoardMember[0].BoardRole)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BoardMember[0].Image)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.BoardMember) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BoardRole)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Image)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</div>